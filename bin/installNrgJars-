set -x

SPOT=~/dev/vendor/lib/samsix
VERSION=4.0.5

cd $S6_TOP/lib
mv nrg*.jar $SPOT

mv $SPOT/nrg_config.jar 	$SPOT/nrg_config-$VERSION.jar
mv $SPOT/nrg_http.jar 		$SPOT/nrg_http-$VERSION.jar
mv $SPOT/nrg.jar 		$SPOT/nrg-$VERSION.jar
mv $SPOT/nrg_ui.jar 		$SPOT/nrg_ui-$VERSION.jar
mv $SPOT/nrg_cust_oru.jar	$SPOT/nrg_cust_oru-$VERSION.jar
mv $SPOT/nrg_cust_bug.jar	$SPOT/nrg_cust_bug-$VERSION.jar

cd $S6_TOP
removeAllSignatures
